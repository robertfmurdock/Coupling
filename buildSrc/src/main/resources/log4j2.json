{
  "Configuration": {
    "Appenders": {
      "Console": {
        "JsonLayout": {
          "keyValuePair": [{
            "key": "timestamp",
            "value": "$${date:yyyy-MM-dd'T'HH:mm:ss.SSSZ}"
          },{
            "key": "testRunIdentifier",
            "value": "$${sys:testRunIdentifier}"
          }],
          "includeTimeMillis": true,
          "eventEol": true,
          "compact": true,
          "objectMessageAsJsonObject": true
        },
        "name": "Console",
        "target": "SYSTEM_OUT"
      },
      "File": {
        "JsonLayout": {
          "keyValuePair": [{
            "key": "timestamp",
            "value": "$${date:yyyy-MM-dd'T'HH:mm:ss.SSSZ}"
          },{
            "key": "testRunIdentifier",
            "value": "$${sys:testRunIdentifier}"
          }],
          "includeTimeMillis": true,
          "eventEol": true,
          "compact": true,
          "objectMessageAsJsonObject": true
        },
        "name": "File",
        "fileName": "build/test-output/test.jsonl",
        "createOnDemand": true
      },
      "Socket": {
        "JsonLayout": {
          "keyValuePair": [{
            "key": "timestamp",
            "value": "$${date:yyyy-MM-dd'T'HH:mm:ss.SSSZ}"
          },{
            "key": "testRunIdentifier",
            "value": "$${sys:testRunIdentifier}"
          }],
          "includeTimeMillis": true,
          "eventEol": true,
          "compact": true,
          "objectMessageAsJsonObject": true
        },
        "name": "Socket",
        "host": "localhost",
        "port": 5555,
        "immediateFail": true,
        "ignoreExceptions": true
      }
    },
    "Loggers": {
      "Root": {
        "AppenderRef": [
          {
            "ref": "File"
          },
          {
            "ref": "Socket"
          }
        ],
        "level": "trace"
      }
    }
  }
}